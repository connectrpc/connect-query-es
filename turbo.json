{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "tsconfig.base.json",
    ".eslintrc.js",
    "package.json",
    "scripts/*"
  ],
  "tasks": {
    "clean": {
      "outputs": ["dist/**"]
    },
    "build": {
      "dependsOn": ["clean", "^build"],
      "outputs": ["dist/**"]
    },
    "generate": {
      "dependsOn": ["^build", "build"]
    },
    "test": {
      "dependsOn": ["generate", "^generate"],
      "cache": false
    },
    "attw": {
      "dependsOn": ["build"]
    },
    "format": {
      "dependsOn": ["generate"]
    }
  }
}
