{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["tsconfig.base.json", ".eslintrc.js", "package.json", "scripts/*"],
  "pipeline": {
    "clean": {
      "outputs": ["dist/**"]
    },
    "build": {
      "dependsOn": ["clean", "^build"],
      "outputs": ["dist/**"]
    },
    "generate": {
      "dependsOn": ["^build", "build"]
    },
    "test": {
      "dependsOn": ["generate"],
      "cache": false
    },
    "format": {
      "dependsOn": ["generate"]
    }
  }
}
